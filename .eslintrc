{
    "extends": "airbnb",
    "env": {
      "node": true,
      "es6": true,
      "jest/globals": true
    },
    "rules": {
      "comma-dangle": 0,
      "import/no-extraneous-dependencies": 1,
      "no-plusplus": 0,
      "no-restricted-syntax": 0,
      "class-methods-use-this": 0,
      "no-bitwise": 1
    },
    "plugins": [
      "jest"
    ]
}
